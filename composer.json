{
    "name": "tanyudii/yin-core",
    "description": "The core services of Yin Project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tanyudii",
            "email": "y.hertanto17@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "tanyudii\\YinCore\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "tanyudii\\YinCore\\YinCoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
